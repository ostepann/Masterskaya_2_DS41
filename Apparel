eset_index()\n\n# Переименовываем столбцы для удобства\npurchase_dates.columns = ['client_id', 'first_purchase_date', 'last_purchase_date']\n\n# Объединяем с df_ap_target_binary по client_id\ndf_ap_target_binary = pd.merge(df_ap_target_binary, purchase_dates, on='client_id', how='left')\n\n# Заполнение NaN значений в датах 1970-01-01\ndf_ap_target_binary['first_purchase_date'] = df_ap_target_binary['first_purchase_date'].fillna(pd.Timestamp('1970-01-01'))\ndf_ap_target_binary['last_purchase_date'] = df_ap_target_binary['last_purchase_date'].fillna(pd.Timestamp('1970-01-01'))\n\n# Создаём новые столбцы с числом дней после первой и последней покупки\ndf_ap_target_binary['days_after_first_purchase'] = (extraction_date - df_ap_target_binary['first_purchase_date']).dt.days\ndf_ap_target_binary['days_after_last_purchase'] = (extraction_date - df_ap_target_binary['last_purchase_date']).dt.days\n\n# Проверим результат\ndisplay(df_ap_target_binary.head())\n","metadata":{"trusted":true,"execution":{"iopub.status.busy":"2025-02-02T14:26:48.143033Z","iopub.execute_input":"2025-02-02T14:26:48.143477Z","iopub.status.idle":"2025-02-02T14:26:48.220204Z","shell.execute_reply.started":"2025-02-02T14:26:48.143444Z","shell.execute_reply":"2025-02-02T14:26:48.218883Z"}},"outputs":[{"output_type":"display_data","data":{"text/plain":"             client_id  target  send_email  send_mobile_push  \\\n0  1515915625468060902       0         126                 0   \n1  1515915625468061003       1         154                 0   \n2  1515915625468061099       0         157                58   \n3  1515915625468061100       0           0               267   \n4  1515915625468061170       0         164                79   \n\n   open_mobile_push  click_mobile_push first_message_date last_message_date  \\\n0                 0                  0         2022-05-19        2024-01-19   \n1                 0                  0         2022-05-20        2024-02-13   \n2                34              