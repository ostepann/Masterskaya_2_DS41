       mask = (df[df_fld] > low) & (df[df_fld] < high)\n        ax = df[mask].boxplot(column=[df_fld], vert=False)\n\n    else:\n        ax = df.boxplot(column=[df_fld], vert=False)\n    # Настройка подписей осей и заголовка\n    plt.title(\"Boxplot для столбца \" + chart_title + \"(\"+ df_fld+\")\")\n    plt.xlabel(chart_title)\n    plt.ylabel(\"Значение\")\n\n    # Убираем название на оси Y\n    plt.gca().set_yticklabels([])\n\n    # Показать график\n    plt.show()\n\n# Построение гистограммы\ndef hist_draw(df, df_fld, low = -1, high = -1, bins = -1, chart_title = '---'):    \n\n    plt.figure(figsize=(15, 5))\n    mask = (df[df_fld] >= low) & (df[df_fld] <= high)\n\n        # Построение гистограммы\n    if (low != -1) and (high != -1):\n        if bins == -1:\n            ax = df[mask][df_fld].hist(bins=int(high - low))\n        else:\n            ax = df[mask][df_fld].hist(bins=bins)\n    else:\n        if bins == -1:\n            ax = df[df_fld].hist(bins=100)\n        else:\n            ax = df[df_fld].hist(bins=bins)\n\n        # Разворот меток по оси X на 20 градусов\n    plt.xticks(rotation=20)\n    plt.ylabel(\"Частота\")\n    plt.title(\"Гистограмма для поля \" + chart_title)\n    # Разворот меток по оси X на 20 градусов\n    plt.xlabel(chart_title)\n        # Отображение графика\n    plt.show()\n\n   # расчет значения \"усов\"\ndef whisker_calc(df, df_fld):    \n    q1, median, q3 = np.percentile(df[df_fld], [25, 50, 75])\n    iqr = q3 - q1\n\n    # Расчёт границ усов\n    upper_whisker = q3 + 1.5 * iqr\n    lower_whisker = q1 - 1.5 * iqr\n\n    # Ограничение усов реальными минимумом и максимумом\n    upper_whisker = min(upper_whisker, df[df_fld].max())  #.astype(np.float64)\n    lower_whisker = max(lower_whisker, df[df_fld].min())  #.astype(np.float64)\n\n    print(f\"Нижняя граница уса: {lower_whisker}\")\n    print(f\"Верхняя граница уса: {upper_whisker}\")\n    \n    full_size = df.shape[0]\n    mask = (df[df_fld] >= lower_whisker) & (df[df_fld] <= upper_whisker)\n    print(\"Всего записей:\",full_size,'\\nДанные без учета выбросов:', df[mask].shape[0],  \n          \"\\nПроцент данных без выбросов:{:.2f}%\".format(100*df[mask].shape[0]/full_size))\n\n    \n    \n    return lower_whisker, upper_whisker\n\ndef period_hist(df, date_fld, chart_title):\n    \n    full_size = df.shape[0]\n    lower_whisker, upper_whisker = date_boxplot(df, date_fld, chart_title)\n#    delta = (df[date_fld].max().date() - df[date_fld].min().date()).days\n    delta = int((upper_whisker - lower_whisker).days/30)\n    print(\"delta=\",delta, \"lower_whisker\", lower_whisker.strftime(\"%Y_%m\"), \"upper_whisker\",upper_whisker.strftime(\"%Y_%m\"))\n    print(\"months=\",relativedelta(upper_whisker, lower_whisker).months)\n    \n    df_fld = df[date_fld][(df[date_fld] >= lower_whisker) & (df[date_fld] < upper_whisker)]\n\n    print(\"Вся выборка: \",full_size,'\\nДанные без учета выбросов:', df_fld.size,  \n          \"\\nПроцент данных без выбросов: {:.2f}%\".format(100*df_fld.size/full_size))\n    #df_fld.info()\n    print('Период без выбросов в днях:', delta)\n    plt.figure(figsize=(15, 7))\n\n    # Построение гистограммы\n#    ax = df_fld[date_fld].hist(bins=delta)\n    ax = df_fld.hist(bins=delta)\n\n    plt.title(\"Гистограмма для поля \" + chart_title)\n    # Разворот меток по оси X на 20 градусов\n    plt.xticks(rotation=20)\n    plt.xlabel(chart_title)\n\n    # Отображение графика\n    plt.show()\n\n# # Построение графика \"ящик с усами\" для полей datetime\ndef date_boxplot(df, date_fld, chart_title):\n    # Преобразование значений в порядковый номер дня\n    df[date_fld + '_num'] = df[date_fld].apply(lambda x: x.toordinal()-719162)\n\n    # Построение boxplot\n    plt.figure(figsize=(15, 2))\n    ax = df.boxplot(column=[date_fld + '_num'], patch_artist=True, vert=False)\n\n    # Установка формата меток на оси X\n    ax.xaxis.set_major_formatter(mdates.DateFormatter('%Y-%m-%d'))\n    ax.xaxis.set_major_locator(mdates.AutoDateLocator())\n\n    # Заголовки\n    plt.title(f\"Ящик с усами для поля {chart_title}\")\n    plt.xlabel(chart_title)\n    plt.ylabel(\"Значение\")\n    plt.gca().set_yticklabels([])  # Убираем название на оси Y\n\n    plt.show()\n\n    # Квантили и межквартильный размах\n    q1, median, q3 = np.percentile(df[date_fld + '_num'], [25, 50, 75])\n    iqr = q3 - q1\n\n    # Границы усов\n    upper_whisker = min(q3 + 1.5 * iqr, df[date_fld + '_num'].max())  \n    lower_whisker = max(q1 - 1.5 * iqr, df[date_fld + '_num'].min())  \n\n    # Переводим в даты\n    lower_whisker_days = datetime(1970, 1, 1) + timedelta(days=int(lower_whisker))\n    upper_whisker_days = datetime(1970, 1, 1) + timedelta(days=int(upper_whisker))\n\n    return lower_whisker_days, upper_whisker_days\n\ndef remove_prefix(d, prefix):\n    return {k[len(prefix):]: v for k, v in d.items() if k.startswith(prefix)}","metadata":{"trusted":true,"execution":{"iopub.status.busy":"2025-02-02T14:19:42.440302Z","iopub.execute_input":"2025-02-02T14:19:42.440718Z","iopub.status.idle":"2025-02-02T14:19:42.472777Z","shell.execute_reply.started":"2025-02-02T14:19:42.440687Z","shell.execute_reply":"2025-02-02T14:19:42.470524Z"}},"outputs":[],"execution_count":428},{"cell_type":"markdown","source":"# Схема данных","metadata":{}},{"cell_type":"code","source":"# Вызовем функцию для отображения диаграммы\ncreate_data_schema_diagram_matplotlib()","metadata":{"trusted":true,"execution":{"iopub.status.busy":"2025-02-02T14:19:42.474519Z","iopub.execute_input":"2025-02-02T14:19:42.475110Z","iopub.status.idle":"2025-02-02T14:19:42.782786Z","shell.execute_reply.started":"2025-02-02T14:19:42.475063Z","shell.execute_reply":"2025-02-02T14:19:42.781120Z"}},"outputs":[{"output_type":"display_data","data":{"text/plain":"<Figure size 1500x800 with 1 Axes>","image/png":"\n"},"metadata":{}}],"execution_count":429},{"cell_type":"markdown","source":"# Анализ данных\n## Загрузка **df_ap_purchases**","metadata":{}},{"cell_type":"code","source":"